
# DO NOT EDIT BELOW THIS LINE
[push]
    default = tracking
[branch]
    autosetupmerge = always
[color]
    diff = auto
    status = auto
    branch = auto
[core]
    excludesfile = ~/.gitignore
[format]
    pretty = %Cblue%h%Creset %Cgreen[%ar]%Creset (%an) %s
[alias]
    c = commit
    a = add
    di = diff
    dic = diff --cached
    pl = pull
    ps = push
    br = branch
    co = checkout
    cl = clone
    st = status
    out = log origin..HEAD
    ca = commit -a
    cam = commit -a -m
    cm = commit -m
    amend = commit --amend -m
    deletelast = reset --hard HEAD~1
    graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative

    plre = pull --rebase
    #qpl = log --pretty=\"%Cblue%h%Creset %Cgreen[%cr]%Creset %s\" origin/production..origin/master
    pr = remote prune origin
    #up = !git fetch origin && git rebase origin/master
    #rbc = rebase --continue
    #rbi = rebase -i
    #mm = !test `git rev-parse master` = $(git merge-base HEAD master) && git checkout master && git merge HEAD@{1} || echo "Non-fastforward"
